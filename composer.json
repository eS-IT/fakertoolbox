{
    "name": "esit/fakertoolbox",
    "description": "Bei dieser Software handelt es sich um eine Erweiterung für das Open Source CMS Contao, die es erlaubt direkt im DCA\ndie Definition für Testdaten zu hinterlegen.",
    "license": "LGPL-3.0-or-later",
    "type": "contao-bundle",
    "authors": [
        {
            "name": "Patrick Froch",
            "email": "info@easySolutionsIT.de",
            "homepage": "http://easySolutionsIT.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@easySolutionsIT.de"
    },
    "require": {
        "php": "^7.2||^8.0",
        "contao/core-bundle": "^4.4",
        "contao/manager-bundle": "^4.4",
        "contao/conflicts": "@dev",
        "fakerphp/faker": "^1.17"
    },
    "require-dev": {
        "contao/test-case": "^4.4",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Esit\\Fakertoolbox\\": ""
        }
    },
    "extra": {
        "contao-manager-plugin": "Esit\\Fakertoolbox\\Classes\\Contao\\Manager\\Plugin"
    }
}
